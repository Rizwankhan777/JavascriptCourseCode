
const perc = 50;
if(perc >= 80 && perc <= 100){
    document.write("you are in merit");
}else if(perc >= 65 && perc < 80){
    document.write("you are in 1st division");
}else if(perc >= 55 && perc < 65){
    document.write("you are in 2nd didivsion");
}else if(perc >= 45 && perc < 55){
    document.write("you are in 3rd div");
}
else {
   document.write("please valid identity");
}
